{
  "expo": {
    "name": "Clear Tape",
    "slug": "cleartape",
    "icon": "./assets/icon.png",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nicktrettel.cleartape"
    },
    "android": {
      "package": "com.nicktrettel.cleartape",
      "permissions": []
    }
  }
}
